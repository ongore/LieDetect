{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "lib": [
      "es2019",
      "dom"
    ],
    "allowJs": true,
    "jsx": "react-native",
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "./types"
    ]
  },
  "include": [
    "App.tsx",
    "src",
    "types"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
